        <owl:disjointWith rdf:resource="&riverbasin;Dairy"/>
        <owl:disjointWith rdf:resource="&riverbasin;Meat_processor"/>
        <owl:disjointWith rdf:resource="&riverbasin;Oil"/>
        <owl:disjointWith rdf:resource="&riverbasin;Slaughterhouse_s"/>
