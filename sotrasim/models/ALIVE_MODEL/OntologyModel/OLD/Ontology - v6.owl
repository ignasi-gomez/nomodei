    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#hasLocation -->

    <owl:ObjectProperty rdf:about="&riverbasin;hasLocation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
