    <owl:ObjectProperty rdf:about="&riverbasin;treats">
        <rdfs:domain rdf:resource="&riverbasin;WWTreatement"/>
        <rdfs:range rdf:resource="&riverbasin;WaterComposition"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#XCoordinate -->

    <owl:DatatypeProperty rdf:about="&riverbasin;XCoordinate">
        <rdfs:domain rdf:resource="&riverbasin;Location"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#YCoordinate -->

    <owl:DatatypeProperty rdf:about="&riverbasin;YCoordinate">
        <rdfs:domain rdf:resource="&riverbasin;Location"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


