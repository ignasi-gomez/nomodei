        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#hasArea -->

    <owl:ObjectProperty rdf:about="&riverbasin;hasArea">
        <rdfs:range rdf:resource="&riverbasin;Area"/>
