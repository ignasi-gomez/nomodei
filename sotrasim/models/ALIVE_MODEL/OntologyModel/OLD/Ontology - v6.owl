    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Abrogation -->

    <owl:Class rdf:about="&riverbasin;Abrogation">
        <rdfs:subClassOf rdf:resource="&riverbasin;Contraction"/>
        <rdfs:comment>Removes an element. Previous instantiations remain unaffected</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#AcetylsalicylicAcid -->

    <owl:Class rdf:about="&riverbasin;AcetylsalicylicAcid">
        <rdfs:subClassOf rdf:resource="&riverbasin;AnalgesicAntiinflammatory"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#ActivationCondition -->

    <owl:Class rdf:about="&riverbasin;ActivationCondition">
        <rdfs:subClassOf rdf:resource="&riverbasin;NormCondition"/>
        <rdfs:comment>When this condition holds true, the norm starts to be checked</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Agriculture -->

    <owl:Class rdf:about="&riverbasin;Agriculture">
        <rdfs:subClassOf rdf:resource="&riverbasin;LandUse"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Aluminium -->

    <owl:Class rdf:about="&riverbasin;Aluminium">
        <rdfs:subClassOf rdf:resource="&riverbasin;HeavyMetal"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Ammonia -->

    <owl:Class rdf:about="&riverbasin;Ammonia">
        <rdfs:subClassOf rdf:resource="&riverbasin;NitrogenCompounds"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#AnalgesicAntiinflammatory -->

    <owl:Class rdf:about="&riverbasin;AnalgesicAntiinflammatory">
        <rdfs:subClassOf rdf:resource="&riverbasin;Emerging_pollutant"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Annulment -->

    <owl:Class rdf:about="&riverbasin;Annulment">
        <rdfs:subClassOf rdf:resource="&riverbasin;Contraction"/>
        <rdfs:comment>Removes an element and all the previous instances of the element. Repairs performed due to element violations might trigger compensations</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Antibiotics -->

    <owl:Class rdf:about="&riverbasin;Antibiotics">
        <rdfs:subClassOf rdf:resource="&riverbasin;Emerging_pollutant"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#Area -->

    <owl:Class rdf:about="&riverbasin;Area"/>
    


