    <owl:ObjectProperty rdf:about="&riverbasin;isPerformed">
        <rdfs:range rdf:resource="&riverbasin;WWTP"/>
        <rdfs:domain rdf:resource="&riverbasin;WWTreatement"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#isProcessOf -->

    <owl:ObjectProperty rdf:about="&riverbasin;isProcessOf">
        <rdfs:range rdf:resource="&riverbasin;Conveyor"/>
        <rdfs:domain rdf:resource="&riverbasin;ConveyorTransporting"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#isTransportingWWOf -->

    <owl:ObjectProperty rdf:about="&riverbasin;isTransportingWWOf">
