        <rdfs:range rdf:resource="&riverbasin;RiverSection"/>
        <rdfs:range rdf:resource="&riverbasin;WWTP"/>
        <rdfs:range rdf:resource="&riverbasin;WaterRetainingTank"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#transforms -->

    <owl:ObjectProperty rdf:about="&riverbasin;transforms">
        <rdfs:domain rdf:resource="&riverbasin;WWTransforming"/>
