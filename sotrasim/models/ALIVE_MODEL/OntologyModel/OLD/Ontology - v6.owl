    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#hasCost -->

    <owl:DatatypeProperty rdf:about="&riverbasin;hasCost">
        <rdfs:domain rdf:resource="&riverbasin;Cost"/>
        <rdfs:range rdf:resource="&xsd;long"/>
    </owl:DatatypeProperty>
    


