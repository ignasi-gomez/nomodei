    <owl:ObjectProperty rdf:about="&riverbasin;isPartOfWWTP">
        <rdf:type rdf:resource="&owl;AsymmetricProperty"/>
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&riverbasin;WWTP"/>
        <rdfs:domain rdf:resource="&riverbasin;WWTreatmentParts"/>
    </owl:ObjectProperty>
    


