        <rdfs:range rdf:resource="&xsd;long"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/riverbasin/ontologies/2013/11/riverbasin#waterFlow -->

    <owl:DatatypeProperty rdf:about="&riverbasin;waterFlow">
